<view>
<switch type="switch" bindchange="change" id="noremind" checked="{{status1}}">不提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="fivem" checked="{{status2}}">会面开始前5分钟提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="tenm" checked="{{status3}}">会面开始前10分钟提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="twentym" checked="{{status4}}">会面开始前20分钟提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="thirtym" checked="{{status5}}">会面开始前30分钟提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="oneh" checked="{{status6}}">会面开始前一个小时提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="twoh" checked="{{status7}}">会面开始前两个小时提醒</switch>
</view>
<button bindtap="confirm" type="primary">确定</button>